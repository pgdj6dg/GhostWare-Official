local RF=loadstring(game:HttpGet("[https://sirius.menu/rayfield"))(](https://sirius.menu/rayfield"))())
local function makeWindow(label)
  return RF:CreateWindow({
    Name="Game Scripts",
    Icon=0,
    LoadingTitle="Loader",
    LoadingSubtitle="pgdj6dg",
    ShowText=label or "Rayfield",
    Theme="Default",
    ToggleUIKeybind="K",
    DisableRayfieldPrompts=false,
    DisableBuildWarnings=false,
    ConfigurationSaving={Enabled=false,FolderName=nil,FileName="Cfg"},
    Discord={Enabled=false,Invite="",RememberJoins=true},
    KeySystem=false,
    KeySettings={Title="",Subtitle:"",Note:"",FileName:"",SaveKey=false,GrabKeyFromSite=false,Key={" "}}
  })
end

local Window=makeWindow("Rayfield")

local ALLOW={
  "[https://raw.githubusercontent.com/pgdj6dg/](https://raw.githubusercontent.com/pgdj6dg/)",
  "[https://raw.githubusercontent.com/namzikavini/](https://raw.githubusercontent.com/namzikavini/)",
  "[https://api.luarmor.net/](https://api.luarmor.net/)",
  "[https://raw.githubusercontent.com/VapeVoidware/](https://raw.githubusercontent.com/VapeVoidware/)",
  "[https://zaphub.xyz/](https://zaphub.xyz/)",
  "[https://raw.githubusercontent.com/bloodball/](https://raw.githubusercontent.com/bloodball/)",
  "[https://raw.githubusercontent.com/LeoKholYt/](https://raw.githubusercontent.com/LeoKholYt/)"
}

local function allowed(u)
  for _,p in ipairs(ALLOW) do
    if u:sub(1,#p)==p then return true end
  end
